{
  "name": "por-quinho",
  "version": "0.0.1",
  "type": "module",
  "description": "De pouco em pouco se enche o porquinho - Aplicação moderna de gestão financeira pessoal",
  "scripts": {
    "dev": "vite",
    "build": "npm run lint:check && npm run test:pre-build && vite build",
    "build:unsafe": "vite build",
    "preview": "vite preview",
    "prod:build:test": "npm run build && vite preview",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:coverage": "playwright test --coverage",
    "lint": "eslint src --ext .vue,.js,.ts",
    "lint:check": "eslint src --ext .vue,.js,.ts --max-warnings 0",
    "lint:fix": "eslint src --ext .vue,.js,.ts --fix",
    "lint:vue": "eslint src/**/*.vue",
    "lint:vue:fix": "eslint src/**/*.vue --fix",
    "format": "prettier --write \"src/**/*.{vue,js,ts,json,css,scss,html}\"",
    "format:check": "prettier --check \"src/**/*.{vue,js,ts,json,css,scss,html}\"",
    "format:vue": "prettier --write \"src/**/*.vue\"",
    "lint:format": "npm run lint:fix && npm run format"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.0.0",
    "@supabase/supabase-js": "^2.57.4",
    "@vee-validate/zod": "^4.15.1",
    "chart.js": "^4.3.0",
    "pinia": "^3.0.3",
    "vee-validate": "^4.15.1",
    "vue": "^3.3.4",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@playwright/test": "^1.56.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.14",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.4.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.27",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^1.2.0"
  }
}
